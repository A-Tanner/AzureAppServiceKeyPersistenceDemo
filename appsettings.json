{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.AspNetCore.DataProtection": "Information"
    }
  },

  "ConnectionStrings": {
    "Database": "<YourDatabaseConnectionString>",
    "DataStorage": "<YourAzureDataStorageConnectionString>"
  },

  "DataProtection": {
    "KeyVaultUri": "<YourKeyVaultUri>",
    "DataStorageContainerName": "<YourContainerName>",
    "BlobName": "keys.xml"
  },


  "AllowedHosts": "*"
}
