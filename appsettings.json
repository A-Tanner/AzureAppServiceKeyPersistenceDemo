{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.AspNetCore.DataProtection": "Information"
    }
  },

  "ConnectionStrings": {
    "Database": "<YourDatabaseConnectionString>",
    "StorageAccount": "<YourAzureStorageAccountConnectionString>"
  },

  "DataProtection": {
    "KeyUri": "<YourKeyUri>",
